<script>
$(function(){
    $('#dowebok').fullpage({
        anchors: ['page1', 'page2', 'page3'],
        'navigation': true
    });

    $(window).resize(function(){
        autoScrolling();
    });

    function autoScrolling(){
        var $ww = $(window).width();
        if($ww < 1024){
            $.fn.fullpage.setAutoScrolling(false);
        } else {
            $.fn.fullpage.setAutoScrolling(true);
        }
        
    }

    autoScrolling();
    
    $(".m_30 ul li").click(function(){
    	$(".m_30 ul li").removeClass("active");
    	$(this).addClass("active");
    	$(".section3_text").find(".section3_table").hide();
    	$(".section3_text").find(".section3_table").eq($(this).index()).show();
    })
    
});
</script>
