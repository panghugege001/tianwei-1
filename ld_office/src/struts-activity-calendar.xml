<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="activityCalendar" extends="struts-default" namespace="/activityCalendar">
        <global-results>
            <result name="error">/office/functions/activityCalendar_error.jsp</result>
            <result name="success">/office/functions/activityCalendar_success.jsp</result>
            <result name="logout">/office/logout.jsp</result>
        </global-results>

        <action name="query" class="ActivityCalendarClass" method="queryAll">
            <result>/office/functions/activityCalendar.jsp</result>
        </action>

        <action name="add" class="ActivityCalendarClass" method="addActivityCalendar">
            <result name="input">/office/functions/activityCalendar_add.jsp</result>
            <result name="success" type="redirectAction">query</result>
        </action>

        <action name="add_view" class="ActivityCalendarClass" method="add_view">
            <result name="input">/office/functions/activityCalendar_add.jsp</result>
        </action>

        <action name="switchFlag" class="ActivityCalendarClass" method="switchActivityCalendar">
            <result type="redirectAction">query</result>
        </action>

        <action name="edit" class="ActivityCalendarClass" method="editActivityCalendar">
            <result name="input">/office/functions/activityCalendar_edit.jsp</result>
        </action>

        <action name="byid" class="ActivityCalendarClass" method="queryActivityCalendarById">
            <result>/office/functions/activityCalendar_edit.jsp</result>
        </action>
    </package>
    
</struts>