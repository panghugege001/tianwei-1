<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.nnti.office.dao.constant.ConstantDao">
	
	<select id="getConstantValue" parameterType="string" resultType="string">
		select
			value
		from
			const
		where
			id = #{id}
	</select>

</mapper>