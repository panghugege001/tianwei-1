<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="mgs" extends="struts-default" namespace="/mgs">
		<action name="updateMGFreeGames" class="mgsAction" method="updateMGFreeGames"></action>
		<action name="queryMGFreeGames" class="mgsAction" method="queryMGFreeGames">
			<result name="input">/office/functions/mgFreeGameList.jsp</result>
		</action>		
		<action name="addPlayersToFreegame" class="mgsAction" method="addPlayersToFreegame"></action>
		<action name="getPlayersByFreegame" class="mgsAction" method="getPlayersByFreegame">
			<result name="input">/office/functions/mgplayersOfFreeGame.jsp</result>
		</action>
		<action name="removePlayersFromFreegame" class="mgsAction" method="removePlayersFromFreegame"></action>
		
		<action name="repairMGDataToKPI" class="mgsAction" method="repairMGDataToKPI"></action>
	</package>
</struts>