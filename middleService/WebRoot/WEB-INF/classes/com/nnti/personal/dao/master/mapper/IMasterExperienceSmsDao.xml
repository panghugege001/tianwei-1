<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nnti.personal.dao.master.IMasterExperienceSmsDao">


	<insert id="insert" parameterType="ExperienceSms">
		INSERT
		INTO
		experience_sms
		(
			<trim suffixOverrides=",">
				<if test="phone != null">phone,</if>
				<if test="ipAddress != null">ip_address,</if>
				<if test="gatewayId != null">gateway_id,</if>
				<if test="createTime != null">create_time,</if>
				<if test="sendTime != null">send_time,</if>
				<if test="smsContent != null">sms_content,</if>
			</trim>
		)
		VALUES
		(
			<trim suffixOverrides=",">
				<if test="phone != null">#{phone},</if>
				<if test="ipAddress != null">#{ipAddress},</if>
				<if test="gatewayId != null">#{gatewayId},</if>
				<if test="createTime != null">#{createTime},</if>
				<if test="sendTime != null">#{sendTime},</if>
				<if test="smsContent != null">#{smsContent},</if>
			</trim>
		)
		<selectKey resultType="java.lang.Integer" keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID
		</selectKey>
	</insert>

	<update id="update" parameterType="ExperienceSms">
		update experience_sms set sms_content=#{smsContent} where phone=#{phone}
	</update>
</mapper>