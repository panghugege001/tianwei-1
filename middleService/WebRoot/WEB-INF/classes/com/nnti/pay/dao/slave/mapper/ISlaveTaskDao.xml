<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nnti.pay.dao.slave.ISlaveTaskDao">


    <sql id="columns_sql">
		id as id,
		pno as pno,
		level as level,
		flag as flag,
		agreeTime as agreeTime,
		agreeIp as agreeIp,
		manager as manager
	</sql>

    
    
    <select id="get" parameterType="java.lang.String" resultType="Task">
    	select
        <include refid="columns_sql"/>
        from
         task
        <where>
        	 <if test="pno != null">pno = #{0}</if>
        	  <if test="level != null">level = #{1}</if>
        </where>
    </select>
    
       <select id="getTask" parameterType="java.lang.String" resultType="Task">
        select
        <include refid="columns_sql"/>
        from
        task
        <where>
            <if test="pno != null">pno = #{pno}</if>
            <if test="level != null">AND level = #{level}</if>
        </where>
    </select>
    
    
    
    

    
	
</mapper>