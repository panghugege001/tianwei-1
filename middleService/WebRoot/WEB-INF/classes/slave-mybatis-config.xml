		<mapper resource="com/nnti/common/dao/slave/mapper/ISlavePreferentialRecordDao.xml" />
		<mapper resource="com/nnti/common/dao/slave/mapper/ISlaveProposalDao.xml" />
		<mapper resource="com/nnti/common/dao/slave/mapper/ISlaveProposalExtendDao.xml" />
