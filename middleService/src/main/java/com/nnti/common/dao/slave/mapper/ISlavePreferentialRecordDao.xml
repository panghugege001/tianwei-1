<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nnti.common.dao.slave.ISlavePreferentialRecordDao">
	
	<sql id="columns_sql">
		pno as pno,
		loginname as loginName,
		platform as platform,
		validbet as validBet,
		createtime as createTime,
		type as type
	</sql>
	
	<select id="get" parameterType="java.lang.String" resultType="PreferentialRecord">
		select
        <include refid="columns_sql" />
		from
        preferential_record
		where pno = #{0}
    </select>
</mapper>